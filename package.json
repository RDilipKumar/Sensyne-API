{
  "name": "apitest",
  "version": "2.0.0",
  "description": "WebDriver bindings for Node.js with Cucumber",
  "main": "index.js",
  "scripts": {
    "satisfied": "satisfied --fix --skip-invalid",
    "selenium-postinstall": "selenium-standalone install",
    "selenium-start": "selenium-standalone start",
    "test": "npm run satisfied && wdio test/config/main.local.conf.js",
